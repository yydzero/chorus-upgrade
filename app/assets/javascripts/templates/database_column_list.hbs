{{#each models}}
    <li {{#if model.selected}}class="selected"{{/if}} data-cid='{{model.cid}}'>
        <div class="text right">
            <div class="main">
                {{#if ../view.showDatasetName}}
                    <span class="aliased_name"><span class="letter">{{model.dataset.aliasedName}}</span></span>
                {{/if}}
                <span class="name"><span>{{name}}</span></span>
            </div>
            <div class="summary">{{description}}</div>
        </div>
<!--         <div class="type {{typeClass}}"><span class="left"/><span class="type_name">{{typeString}}</span><span class="right"/></div> TODO delete when final -->
        <div class="type {{typeClass}}"><span class="type_name">{{typeString}}</span></div>
    </li>
{{/each}}
