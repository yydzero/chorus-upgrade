{{#if hasResults}}
    <div class="this_workspace"></div>
    <div class="workfile_list"></div>
    <div class="attachment_list"></div>
    <div class="hdfs_list"></div>
    <div class="dataset_list"></div>
    <div class="data_source_list"></div>
    <div class="hdfs_data_source_list"></div>
    <div class="gnip_data_source_list"></div>
    <div class="workspace_list"></div>
    <div class="user_list"></div>
{{else}}
    <div class="sorry">
        {{#if isTag}}
            {{t "tag.show.no_results"}}
        {{else}}
            <div class="header">{{t "search.no_results.header"}}</div>
            <ul>
                <li>{{t "search.no_results.check_spelling"}}</li>
                <li>{{t "search.no_results.try_wildcards"}}</li>
                {{#if isConstrained}}
                    <li><a href="{{expandHref}}">{{t "search.no_results.expand"}}</a></li>
                {{/if}}
            </ul>
        {{/if}}
    </div>
{{/if}}
