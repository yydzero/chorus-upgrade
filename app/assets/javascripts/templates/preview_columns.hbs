<div class="girdle">

<div class="sub_header">
    <span class="name" title="{{objectName}}">{{objectName}}</span>
    <span class="column_count">({{count}})</span>
    <input class="search" placeholder='{{t "dataset.manage_join_tables.find_a_column"}}'/>
</div>

<div class="list_container">
    <ul class="database_column_list list">
        {{#each models}}
            <li>
                <div class="text">
                    <div class="main">
                        <span class="name" title="{{name}}">{{name}}</span>
                    </div>
                    <div class="details">
                        <span class="comment">{{description}}</span>
                    </div>
                </div>
<!--                 <div class="type {{typeClass}}"><span class="left"/><span class="type_name">{{dataType}}</span><span class="right"/></div> -->
                <div class="type {{typeClass}}"><span class="type_name">{{dataType}}</span></div>
            </li>
        {{/each}}
    </ul>
</div>

</div>
<!-- END girdle -->

<div id="dialog_bottom" class="form_controls">
    <button class="cancel modalish left">{{{t "actions.return_to_list"}}}</button>
</div>
