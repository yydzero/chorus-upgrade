{{#if pagination}}
    <div class="pagination content_details"></div>
{{/if}}
{{#if models.length}}
    <div class="items collection_list">
        <ul>
         {{#each models}}
            <li data-id="{{id}}" class="{{selected}}"><span class="image"><img src="{{imageUrl}}"/></span><span class="name" title="{{name}}">{{name}}</span></li>
         {{/each}}
        </ul>
    </div>
{{else}}
    <div class="none">{{t emptyListTranslationKey}}</div>
{{/if}}
