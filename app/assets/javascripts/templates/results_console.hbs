<div class="sql_errors hidden">
    <ul>
        <li>{{t "results_console_view.there_was_an_error"}}</li>
    </ul>
    <div class="actions">
        <a href="#" class="action view_details">{{t "actions.view_details"}}</a> | <a href="#" class="action close_errors">{{t "actions.close"}}</a>
    </div>
</div>
<div class="execution">
    <div class="right">
        <span class="controls hidden">
            <a href="#" class="action view_details">{{t "actions.view_details"}}</a>
            
            {{#if hasResults}}
                <span class="separator">|</span>
                <a href="#" class="download_csv">{{t "actions.download"}}</a>
            {{/if}}
            
            {{#if enableResize}}
                <span class="separator collapsable">|</span>
                <a href="#" class="minimize hidden">{{t "results_console_view.minimize"}}</a>
                <a href="#" class="maximize hidden">{{t "results_console_view.maximize"}}</a>
            {{/if}}
            
            {{#if enableClose}}
                <span class="separator collapsable close">|</span>
                <a class="close" href='#'>{{t "actions.close"}}</a>
            {{/if}}
        </span>

        <span class='execution_info'>
            <span class="elapsed_time"/>
            <span class="spinner"/>
            <span class="task_loading">{{t "results_console_view.executing"}}</span>
            <span class="separator">|</span>
            <a class="cancel" href='#'>{{t "actions.cancel"}}</a>
        </span>
    </div>
    <h1 class="title">{{t titleKey}}</h1>
</div>
<div class="result_table collapsed"/>
<div class="bottom_gutter hidden">
    {{#if enableExpander}}
    <div class="expander_button">
        <div class="arrow down"></div>
    </div>
    {{/if}}
</div>
